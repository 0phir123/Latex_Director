{
  "manifest_version": 3,
  "name": "Template Extension",
  "description": "Example Description",
  "version": "1.0",
  "action": {
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
  },
  
    "default_title": "Diet Mama Rule the world"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": ["contentScript.js"],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "scripting"
  ]
}
