{
    "manifest_version": 3,
    "name": "Template Extension",
    "description": "Example Description",
    "version": "1.0",
    "icons": {
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "content_scripts": [
      {
        "js": [
          "scripts/contentScript.js"
        ],
        "matches": [
          "http://*/*",
          "https://*/*"
        ]
      }
    ],
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {}
  }